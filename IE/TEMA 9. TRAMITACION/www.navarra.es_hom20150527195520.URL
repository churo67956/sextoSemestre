        function showTab (tab){       
            var $this = $('.info > div#'+tab+'');
            $('.info > div').not($this).hide();
            $($this).show();
            
            $('.tabs > .activa').removeClass("activa").addClass("noactiva");
            $('.tabs > div > a[title="'+tab+'"]').parent().removeClass("noactiva").addClass("activa");
            
            
            if(tab=='Resultados'){
                $('.div_tabs_resultados > div').show();
                $('.tabs > div > a[title="'+tab+'"]').nextAll("p").filter(function(){
                    if ($(this).find('a').html() == 'Resultados') {
                        return false;
                    }
                    return true;
                }).show();
                /*FALTA si el p > a value = Resultado ocultarlo*/
                var len = $('.tabs > div > a[title="'+tab+'"]').nextAll('p').length;
                 
                var $this = $('.bloque_menu_contenido .bloque_contenido').children('div').get(len-1);
                if ($this !=undefined)
                {
                    $('.bloque_menu_contenido .bloque_contenido > div').not($this).hide();
                    $($this).show();
                }
                
                $('.tabs > .activa > p > a').removeClass("activa").addClass("noactiva");
                $('.tabs > .activa > p').removeClass("activa").addClass("noactiva");
                                
                $($('.tabs > div > a[title="'+tab+'"]').nextAll('p').get(len-1)).children().addClass('activa');
                $($('.tabs > div > a[title="'+tab+'"]').nextAll('p').get(len-1)).addClass('activa');
                $('.div_tabs_resultados').removeClass('oculto');
               
            }
            else {
                $('.tabs > .noactiva > p').hide();
            }
        }
        function showSubTab(tab){
   
            var $this = $('.bloque_menu_contenido .bloque_contenido > div#'+tab);
            $('.bloque_menu_contenido .bloque_contenido > div').not($this).hide();
            $($this).show();
            
            $('.tabs > .activa > p > a').removeClass("activa").addClass("noactiva");
            $('.tabs > .activa > p').removeClass("activa").addClass("noactiva");
            $('.tabs > .activa > p > a#'+tab).removeClass("noactiva").addClass("activa");
            $('.tabs > .activa > p > a#'+tab).parent().removeClass("noactiva").addClass("activa");
            
        }
